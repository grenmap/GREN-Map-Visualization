{
  "name": "@grenmap/gren-map-visualization",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
    "merge": "node ./scripts/merge.js",
    "rename": "node ./scripts/rename.js",
    "clean": "node ./scripts/clean.js",
    "package": "node ./scripts/package.js",
    "bundle": "npm run build && npm run merge && npm run clean && npm run package",
    "bundle:dev": "npm run build:dev && npm run merge && npm run clean && npm run package",
    "cp": "find ./dist/gren-map-visualization/ -name 'gren*.js' -exec cp '{}' /vol/dist/ ';'",
    "deploy": "npm run bundle && rm -r /vol/dist/gren-map.*.js || true && npm run cp",
    "deploy:dev": "npm run bundle:dev && rm -r /vol/dist/gren-map.*.js || true && npm run cp",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@apollo/client": "^3.0.0",
    "@types/css-font-loading-module": "^0.0.4",
    "apollo-angular": "^2.2.0",
    "deepmerge": "^4.2.2",
    "graphql": "^15.0.0",
    "leaflet": "^1.7.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.7",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.7",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/language-service": "^15.2.8",
    "@angular/localize": "^15.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.5.21",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "combine-source-map": "^0.8.0",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.8.0"
  }
